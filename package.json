{
  "name": "meshblu-firehose-socket.io",
  "version": "1.7.0",
  "description": "Meshblu Firehose Client for socket.io",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/node-meshblu-firehose-socket.io.git"
  },
  "keywords": [
    "meshblu",
    "meshblu-client",
    "meshblu-firehose"
  ],
  "author": "Octoblu, Inc <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/node-meshblu-firehose-socket.io/issues"
  },
  "homepage": "https://github.com/octoblu/node-meshblu-firehose-socket.io#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "socket.io": "^1.4.5"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "eventemitter2": "^1.0.0",
    "lodash": "^4.7.0",
    "socket.io-client": "^1.4.5"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
